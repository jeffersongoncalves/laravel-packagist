{
  "name": "jeffersongoncalves/laravel-packagist",
  "description": "",
  "keywords": [
    "jeffersongoncalves",
    "laravel",
    "laravel-packagist"
  ],
  "homepage": "https://github.com/jeffersongoncalves/laravel-packagist",
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson Gon√ßalves",
      "email": "gerson.simao.92@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2|^8.3",
    "laravel/framework": "^11.0|^12.0",
    "spatie/laravel-package-tools": "^1.14.0"
  },
  "require-dev": {
    "larastan/larastan": "^3.0",
    "laravel/pint": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "JeffersonGoncalves\\Packagist\\": "src/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JeffersonGoncalves\\Packagist\\PackagistServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
